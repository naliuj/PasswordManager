<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="vBox" prefHeight="326.0" prefWidth="475.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="passwordmanager.MainFXMLController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <ListView fx:id="listView" prefHeight="243.0" prefWidth="475.0" VBox.vgrow="ALWAYS" />
      <AnchorPane prefHeight="53.0" prefWidth="475.0">
         <children>
            <Button fx:id="newButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#newButtonClick" text="New Entry" />
            <Button fx:id="deleteButton" layoutX="399.0" layoutY="14.0" mnemonicParsing="false" onAction="#deleteButtonClick" text="Delete" />
            <Button fx:id="editButton" layoutX="355.0" layoutY="14.0" mnemonicParsing="false" onAction="#editButtonClick" text="Edit" />
         </children>
      </AnchorPane>
   </children>
</VBox>
